{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "forwardingGateway": {
    "requiredHeaders": [
      "Authorization",
      "Cookie"
    ],
    "allowedForwardedHosts": [
      "shyam-jewellers-api-xxx.azurewebsites.net"
    ],
    "allowedForwardedHeaders": [
      "Authorization",
      "Cookie",
      "Content-Type"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [ "anonymous" ]
    }
  ]
}
